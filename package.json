    "report": "node dist/utility/cucumberHtmlReportGenerator.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3"
  },
  "dependencies": {
    "cucumber": "^5.1.0",
    "cucumber-html-reporter": "^4.0.5",
    "supertest": "^4.0.2"
  }
