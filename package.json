{
  "name": "demo-test",
  "version": "1.0.0",
  "description": "Demo API framework using supertest",
  "repository": "git@github.com:MalpaBajpai/demoTest.git",
  "author": "Malpa Bajpai <malpa.bajpai1@gmail.com>",
  "license": "NONE",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist",
